{
  "name": "@metamask/template-snap-monorepo",
  "version": "0.1.0",
  "private": true,
  "description": "",
  "homepage": "https://github.com/MetaMask/template-snap-monorepo#readme",
  "bugs": {
    "url": "https://github.com/MetaMask/template-snap-monorepo/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Galactica-corp/galactica-snap.git"
  },
  "license": "(MIT-0 OR Apache-2.0 OR BUSL-1.1)",
  "author": "",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "yarn workspaces foreach --parallel --topological --verbose run build",
    "build:snap-api": "turbo run build --filter=snap-api",
    "dev:snap": "turbo run dev --filter=snap...",
    "dev:snap-api": "turbo run dev --filter=snap-api...",
    "lint": "yarn lint:eslint && yarn lint:misc --check",
    "lint:eslint": "eslint . --cache --ext js,ts",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:misc --write",
    "lint:misc": "prettier '**/*.json' '**/*.md' '!**/CHANGELOG.md' '!test/**' '**/*.yml' --ignore-path .gitignore",
    "start": "yarn workspaces foreach --parallel --interlaced --verbose run start",
    "test": "yarn workspaces foreach -vp run test"
  },
  "resolutions": {
    "blake-hash": "patch:blake-hash@npm:2.0.0#.yarn/patches/blake-hash-npm-2.0.0-c63b9a2c2d.patch",
    "circomlibjs": "patch:circomlibjs@npm:0.1.7#.yarn/patches/circomlibjs-npm-0.1.7-92fe3b3421.patch",
    "ffjavascript": "patch:ffjavascript@npm:0.2.57#.yarn/patches/ffjavascript-npm-0.2.57-a4801c15bf.patch",
    "web-worker": "patch:web-worker@npm:1.2.0#.yarn/patches/web-worker-npm-1.2.0-ef71df710e.patch"
  },
  "devDependencies": {
    "@metamask/eslint-config": "^11.1.0",
    "@metamask/eslint-config-jest": "^11.1.0",
    "@metamask/eslint-config-nodejs": "^11.1.0",
    "@metamask/eslint-config-typescript": "^11.1.0",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.8.2",
    "eslint-plugin-jsdoc": "^39.2.9",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.2.1",
    "prettier-plugin-packagejson": "^2.2.18",
    "turbo": "^1.10.13",
    "typescript": "^5.0.4"
  },
  "packageManager": "yarn@3.2.1",
  "engines": {
    "node": ">=16.0.0"
  }
}
