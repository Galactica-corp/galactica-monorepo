{
  "name": "galactica-passport-poc",
  "version": "0.4.0",
  "private": true,
  "license": "(MIT-0 OR Apache-2.0)",
  "scripts": {
    "build": "cross-env GATSBY_TELEMETRY_DISABLED=1 gatsby build",
    "clean": "rimraf build",
    "lint": "yarn lint:eslint && yarn lint:misc --check",
    "lint:eslint": "eslint . --cache --ext js,ts",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:misc --write",
    "lint:misc": "prettier '**/*.json' '**/*.md' '!CHANGELOG.md' --ignore-path .gitignore",
    "start": "cross-env GATSBY_TELEMETRY_DISABLED=1 gatsby develop --port 8001",
    "test": "echo \"no tests available for front-end\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@galactica-net/snap-api": "workspace:^",
    "@metamask/providers": "^16.1.0",
    "bignumber.js": "^9.3.0",
    "ethers": "^5.8.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-is": "^18.3.1",
    "styled-components": "5.3.11"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.4.0",
    "@testing-library/dom": "^8.17.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.11.0",
    "gatsby": "^4.25.9",
    "gatsby-plugin-manifest": "^4.25.0",
    "gatsby-plugin-styled-components": "^5.25.0",
    "gatsby-plugin-svgr": "^3.0.0-beta.0",
    "prettier": "^3.5.3",
    "prettier-plugin-packagejson": "^2.5.15",
    "rimraf": "^5.0.10",
    "typescript": "^5.8.3"
  }
}
