{
  "name": "@galactica-net/snap",
  "version": "0.9.0-beta.4",
  "description": "A Metamask Snap for managing and using zkCertificates on the Galactica network.",
  "repository": {
    "type": "git",
    "url": "https://github.com/Galactica-corp/galactica-snap.git"
  },
  "license": "(BUSL-1.1 OR MIT-0 OR Apache-2.0)",
  "main": "src/index.ts",
  "files": [
    "dist/",
    "images/",
    "snap.manifest.json"
  ],
  "scripts": {
    "build": "mm-snap build",
    "build:clean": "yarn clean && yarn build",
    "build:website": "node ./scripts/build-website.js",
    "clean": "rimraf dist",
    "eval:snap": "mm-snap eval -b dist/bundle.js --verboseErrors",
    "lint": "yarn lint:eslint && yarn lint:misc --check",
    "lint:eslint": "eslint . --cache --ext js,ts",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:misc --write",
    "lint:misc": "prettier '**/*.json' '**/*.md' '!CHANGELOG.md' --ignore-path .gitignore",
    "proofPrep": "ts-node src/scripts/proofGenerationPrep.ts \"$@\"",
    "serve": "mm-snap serve",
    "start": "mm-snap watch",
    "test": "yarn run test:unit",
    "test:unit": "mocha --colors -r ts-node/register \"test/*.test.ts\"",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@galactica-net/galactica-types": "workspace:^",
    "@galactica-net/snap-api": "workspace:^",
    "@galactica-net/zk-certificates": "workspace:^",
    "@metamask/eth-sig-util": "^8.2.0",
    "@metamask/snaps-sdk": "^9.0.0",
    "circomlibjs": "^0.1.7",
    "snarkjs": "https://github.com/Galactica-corp/snarkjs-ses.git#v0.5-snap",
    "viem": "^2.34.0"
  },
  "devDependencies": {
    "@iden3/binfileutils": "^0.0.11",
    "@metamask/auto-changelog": "^3.4.4",
    "@metamask/eslint-config": "^12.2.0",
    "@metamask/eslint-config-jest": "^12.1.0",
    "@metamask/eslint-config-nodejs": "^12.1.0",
    "@metamask/eslint-config-typescript": "^12.1.0",
    "@metamask/providers": "^13.1.0",
    "@metamask/rpc-methods": "^3.0.0",
    "@metamask/snaps-cli": "^6.7.0",
    "@metamask/snaps-types": "^3.1.0",
    "@metamask/snaps-ui": "^3.1.0",
    "@metamask/utils": "^8.5.0",
    "@types/chai": "^4.3.20",
    "@types/chai-as-promised": "^7.1.8",
    "@types/circomlibjs": "^0.1.6",
    "@types/mocha": "^10.0.10",
    "@types/object-hash": "^3.0.6",
    "@types/react": "18.2.4",
    "@types/react-dom": "18.2.4",
    "@types/sinon": "^10.0.20",
    "@types/sinon-chai": "^3.2.12",
    "@types/whatwg-url": "^11.0.5",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "buffer": "^6.0.3",
    "chai": "^4.5.0",
    "chai-as-promised": "^7.1.2",
    "chai-fetch-mock": "^3.0.0",
    "eslint": "^9.11.0",
    "fetch-mock": "^9.11.0",
    "ffjavascript": "^0.3.1",
    "js-sha3": "^0.8.0",
    "mocha": "^10.8.2",
    "object-hash": "^3.0.0",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^3.5.3",
    "prettier-plugin-packagejson": "^2.5.15",
    "rimraf": "^5.0.10",
    "sinon": "^15.2.0",
    "sinon-chai": "^3.7.0",
    "ts-command-line-args": "^2.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "whatwg-url": "^14.2.0"
  },
  "packageManager": "yarn@3.2.1",
  "engines": {
    "node": ">=18.6.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
