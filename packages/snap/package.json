{
  "name": "galactica-snap",
  "version": "0.1.0",
  "private": true,
  "description": "A Snap for managing and using zkCerts in Galactica.",
  "repository": {
    "type": "git",
    "url": "https://github.com/Galactica-corp/galactica-snap.git"
  },
  "license": "(MIT-0 OR Apache-2.0)",
  "main": "src/index.ts",
  "files": [
    "dist/",
    "images/",
    "snap.manifest.json"
  ],
  "scripts": {
    "build": "mm-snap build",
    "build:clean": "yarn clean && yarn build",
    "build:website": "node ./scripts/build-website.js",
    "clean": "rimraf dist",
    "lint": "yarn lint:eslint && yarn lint:misc --check",
    "lint:eslint": "eslint . --cache --ext js,ts",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:misc --write",
    "lint:misc": "prettier '**/*.json' '**/*.md' '!CHANGELOG.md' --ignore-path .gitignore",
    "proofPrep": "ts-node src/scripts/proofGenerationPrep.ts",
    "serve": "mm-snap serve",
    "start": "mm-snap watch"
  },
  "dependencies": {
    "circomlibjs": "^0.1.7",
    "snarkjs": "https://github.com/Galactica-corp/snarkjs-ses.git#ses-mm-snap",
    "zkkyc": "git+ssh://git@github.com:Galactica-corp/zkKYC.git#deployment"
  },
  "devDependencies": {
    "@iden3/binfileutils": "^0.0.11",
    "@lavamoat/allow-scripts": "^2.0.3",
    "@metamask/auto-changelog": "^3.1.0",
    "@metamask/eslint-config": "^11.1.0",
    "@metamask/eslint-config-jest": "^11.1.0",
    "@metamask/eslint-config-nodejs": "^11.1.0",
    "@metamask/eslint-config-typescript": "^11.1.0",
    "@metamask/snap-types": "^0.23.0",
    "@metamask/snaps-cli": "^0.27.1",
    "@metamask/utils": "^3.4.1",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "buffer": "^6.0.3",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.8.2",
    "eslint-plugin-jsdoc": "^39.2.9",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.2.1",
    "prettier-plugin-packagejson": "^2.2.11",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "packageManager": "yarn@3.2.1",
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
